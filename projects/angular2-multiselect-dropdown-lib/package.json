{
  "name":"angular2-multiselect-dropdown",
  "version":"14.0.0",
  "license": "MIT",
  "author": "Amir Hossein Khajegan",
  "description": "Angular Multiselect dropdown component library for Angular version 14",
  "keywords": [
    "angular 2 multiselect dropdown",
    "angular multiselect",
    "angular dropdown",
    "angular 14"
  ],
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "allowedNonPeerDependencies": ["@tweenjs/tween.js"],
    "assets": [
      "README.md",
      "LICENSE",
      "./themes/*.theme.scss",
      "./themes/*.theme.css"
    ],
    "dest": "../../dist"
  },
  "peerDependencies": {
    "@tweenjs/tween.js": "^18.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khajegan/angular2-multiselect-dropdown"
  },
  "bugs": {
    "url": "https://github.com/khajegan/angular2-multiselect-dropdown/issues"
  }
}
